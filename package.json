{
  "name": "hot-spot",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next -- -p 3001",
    "build": "next build",
    "start": "next start",
    "generate": "npm -s run generate:prisma && npm -s run generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only -P nexus.tsconfig.json nexus/nexusSchema",
    "prettier-format": "prettier --config .prettierrc '**/*.{js,jsx,ts}' --write",
    "codegen": "graphql-codegen --config codegen.yml",
    "create-migration": "prisma migrate dev --preview-feature",
    "prisma-push": "prisma db push --preview-feature"
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "^0.16.0",
    "@hookform/error-message": "0.0.5",
    "@paljs/nexus": "2.11.0",
    "@prisma/client": "^2.14.0",
    "@theme-ui/presets": "^0.3.5",
    "@types/react-select": "^3.1.2",
    "@xstate/inspect": "^0.4.1",
    "@xstate/react": "^1.2.2",
    "apollo-server-micro": "^2.19.1",
    "cross-env": "^7.0.3",
    "date-fns": "^2.16.1",
    "downshift": "^6.0.10",
    "graphql": "^15.4.0",
    "graphql-codegen-plugin-typescript-swr": "^0.4.0",
    "graphql-iso-date": "^3.6.1",
    "graphql-request": "^3.4.0",
    "graphql-tag": "2.11.0",
    "graphql-type-json": "^0.3.2",
    "isomorphic-unfetch": "^3.1.0",
    "next": "^10.0.4",
    "nexus": "^1.0.0",
    "react": "^17.0.1",
    "react-collapse": "^5.1.0",
    "react-datepicker": "^3.3.0",
    "react-dom": "^17.0.1",
    "react-hook-form": "^6.14.0",
    "react-icons": "^4.1.0",
    "react-select": "^3.1.1",
    "react-swipeable-views": "^0.13.9",
    "reflect-metadata": "^0.1.13",
    "swr": "^0.3.11",
    "theme-ui": "^0.3.5",
    "ts-node": "^9.1.1",
    "uuid": "^8.3.2",
    "xstate": "^4.15.4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.20.0",
    "@graphql-codegen/typescript": "^1.20.0",
    "@graphql-codegen/typescript-graphql-request": "^3.0.0",
    "@graphql-codegen/typescript-operations": "^1.17.13",
    "@prisma/cli": "^2.13.0",
    "@types/graphql-iso-date": "^3.4.0",
    "@types/node": "^14.14.6",
    "@types/react": "^16.9.55",
    "@types/react-collapse": "^5.0.0",
    "@types/react-datepicker": "^3.1.2",
    "@types/theme-ui": "^0.3.7",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "babel-plugin-graphql-tag": "2.5.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "husky": "^4.3.7",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.1.0",
    "typescript": "^4.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --pattern \"**/*.*(ts|tsx|graphql)\" --staged"
    }
  },
  "cacheDirectories": [
    ".next/cache"
  ]
}
